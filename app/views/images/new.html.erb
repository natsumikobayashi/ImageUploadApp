<h1>Images#new</h1>
<%= form_with model: @image, id: "imageForm", local: true do |form| %>
  <div id="name">
    <%= form.label :name %>
    <%= form.text_field :name %>  
  </div>
  <div id="image">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>
  <p><%= form.submit "Upload" %></p>
<% end %>

<%= render "new_validation" %>